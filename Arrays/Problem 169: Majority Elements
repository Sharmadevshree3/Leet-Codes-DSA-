# Leet-Codes-DSA-
class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n=nums.size();
        for(int i=0;i<n;i++)
        {
            int frequency =1;
            for(int j=i+1;j<n;j++)
            {
                if(nums[i]==nums[j])
                {
                    frequency++;
                }
            }
            if(frequency>n/2)
            {
                return nums[i];
            }
        }
        return -1;
    }
};
